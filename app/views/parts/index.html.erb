<h3>パーツデータ一覧</h3>
<table>
  <% @parts.each do |part| %>
  <tr>
    <td><%= part.name%> </td>
    <td><%= part.genre %></td>
    <td><%= part.spec %></td>
    <td><%= part.manufacturer %></td>
    <td><%= part.price %></td>
    <td><%= image_tag part.image, size: "100x100", :alt => "パーツ画像" %></td>
  </tr>
  <% end %>
</table>
